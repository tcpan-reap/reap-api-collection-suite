{
  "type": "module",
  "scripts": {
    "webhook": "node tools/webhook_server/server.js",
    "tunnel": "ngrok http 3001",
    "subscribe:webhook": "node tools/webhook_server/subscribeWebhook.js",
    "start:all": "concurrently -k -s first -n WEBHOOK,TUNNEL \"npm run webhook\" \"npm run tunnel\"",
    "stop:all": "echo \"Use Ctrl+C in the terminal running start:all\"",
    "postman:gen": "node generate_postman_schema/generate_postman_collection.js"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ngrok": "^5.0.0-beta.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.6",
    "@babel/traverse": "^7.28.6"
  }
}
